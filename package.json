{
  "name": "sapper-tempalte-tailwind",
  "description": "the template application made with sapper and tailwind",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:css && sapper build --legacy",
    "build:css": "NODE_ENV=production postcss static/global.css -o static/index.css",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "run-p start:dev watch:css",
    "export": "sapper export --legacy",
    "lint": "eslint . --ext .js,.ts",
    "fix": "eslint . --ext .js,.ts,.svelte --fix",
    "format": "prettier --write --svelte-sort-order scripts-markup-styles ./**/*.svelte",
    "start": "node __sapper__/build",
    "start:dev": "sapper dev",
    "test": "run-p --race dev cy:run",
    "watch:css": "postcss static/global.css -o static/index.css -w"
  },
  "dependencies": {
    "compression": "1.7.4",
    "polka": "next",
    "sanitize.css": "11.0.1",
    "sirv": "1.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.12.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.0",
    "@babel/preset-env": "7.12.0",
    "@babel/runtime": "7.12.0",
    "@rollup/plugin-alias": "3.1.1",
    "@rollup/plugin-babel": "5.1.0",
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "@rollup/plugin-typescript": "5.0.2",
    "@tsconfig/svelte": "1.0.10",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "add": "2.0.6",
    "eslint": "7.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-svelte3": "2.7.3",
    "npm-run-all": "4.1.5",
    "postcss-cli": "7.1.2",
    "postcss-import": "12.0.1",
    "postcss-load-config": "2.1.2",
    "postcss-nested": "4.2.3",
    "prettier": "2.1.2",
    "prettier-plugin-svelte": "1.1.0",
    "rollup": "2.30.0",
    "rollup-plugin-svelte": "5.2.3",
    "rollup-plugin-terser": "6.1.0",
    "sapper": "0.28.10",
    "stylelint": "13.6.1",
    "stylelint-config-recess-order": "2.1.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-declaration-block-no-ignored-properties": "2.3.0",
    "stylelint-no-unsupported-browser-features": "4.0.0",
    "stylelint-order": "4.1.0",
    "svelte": "3.29.0",
    "svelte-check": "1.0.11",
    "svelte-preprocess": "4.5.1",
    "tailwindcss": "1.6.2",
    "typescript": "4.1.0-dev.20200805",
    "yarn": "1.22.10"
  }
}
